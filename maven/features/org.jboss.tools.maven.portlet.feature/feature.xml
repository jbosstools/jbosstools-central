<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.jboss.tools.maven.portlet.feature"
      label="%featureName"
      version="1.8.4.qualifier"
      provider-name="%providerName"
      plugin="org.jboss.tools.maven.portlet"
      license-feature="org.jboss.tools.foundation.license.feature"
	  license-feature-version="0.0.0">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.m2e.feature" version="1.5.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.m2e.wtp.feature" version="1.1.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.feature" version="1.6.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.portlet.feature" version="1.6.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.jboss.tools.maven.portlet"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
