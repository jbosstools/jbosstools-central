      <import plugin="org.dadacoalition.yedit" version="1.0.18" match="greaterOrEqual"/>
      <import plugin="org.yaml.snakeyaml" version="1.14.0" match="greaterOrEqual"/>
   </requires>

   <plugin id="org.jboss.tools.community.central" download-size="0" install-size="0" version="0.0.0" unpack="false"/>

</feature>
